<p id="notice"><%= notice %></p>

<h1>Listing Employees Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees_courses.each do |employees_course| %>
      <tr>
        <td><%= link_to 'Show', employees_course %></td>
        <td><%= link_to 'Edit', edit_employees_course_path(employees_course) %></td>
        <td><%= link_to 'Destroy', employees_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employees course', new_employees_course_path %>
